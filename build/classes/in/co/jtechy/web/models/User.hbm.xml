<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class name="in.co.jtechy.web.models.User" table="user">
    <id column="id" name="id" type="java.lang.Integer">
        </id>
    <property column="name" name="name" type="java.lang.String"/>
    <property column="password" name="password" type="java.lang.String"/>
    <property column="active" name="active" type="java.lang.Boolean"/>
    <property column="create_date" name="createDate" type="java.sql.Timestamp"/>
    <property column="update_date" name="updateDate" type="java.sql.Timestamp"/>
  </class>
</hibernate-mapping>
